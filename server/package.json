{
  "name": "node-backend",
  "version": "1.0.0",
  "description": "Backend Node.js para gestión de taller agrícola",
  "main": "src/index.js",
  "scripts": {
  "dev": "nodemon src/index.js",
  "start": "node src/index.js",
